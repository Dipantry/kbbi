{
  "name": "dipantry/kbbi",
  "description": "A PHP library for searching words in the Indonesian KBBI database",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "rigsto/api-http-status": "^1.1",
    "guzzlehttp/guzzle": "^7.4",
    "ext-libxml": "*",
    "ext-dom": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "orchestra/testbench": "^7.0"
  },
  "authors": [
    {
      "role": "Developer",
      "name": "Auriga Aristo",
      "email": "aurigaaristo@gmail.com",
      "homepage": "https://aurigaaristo.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Dipantry\\Kbbi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dipantry\\Kbbi\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
